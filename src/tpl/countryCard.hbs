{{#each this}}
  <h2 class='contry__title'>{{name}}</h2>
  <div class='country__container'>
    <div class='country__info'>
      <p class='country__info-capital'>Capital:{{capital}}</p>
      <p class='country__info-population'>Population:{{population}}</p>
      <p class='country__info-languages'>Languages:</p>
      <ul class='country__list-lang'>
        {{#each languages}}
          <li>{{name}}</li>
        {{/each}}
      </ul>
    </div>
    <img class='country__flag' src='{{flag}}' alt='{{name}}' />
  </div>
{{/each}}